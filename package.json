{
  "name": "clappr-youtube-playback",
  "version": "0.0.0",
  "main": "dist/clappr-youtube-playback.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "release": "webpack --progress -d --optimize-minimize --optimize-dedupe --output-filename youtube_playback.min.js",
    "build": "webpack --progress",
    "watch": "webpack --progress --watch",
    "lint": "eslint src",
    "test": "karma start --single-run --browsers Chrome",
    "start": "webpack-dev-server --content-base public/ --output-public-path /latest --hot",
    "lock": "rm -rf npm-shrinkwrap.json node_modules && npm install --silent && npm shrinkwrap"
  },
  "dependencies": {
    "clappr": "^0.2.22"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "compass-mixins": "^0.12.7",
    "css-loader": "^0.19.0",
    "eslint": "^3.1.0",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "karma": "^0.13.10",
    "node-sass": "^3.3.3",
    "sass-loader": "^3.0.0",
    "svg-inline-loader": "^0.3.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "description": "Clappr plugin for playing YouTube based videos",
  "bin": {
    "clappr-youtube-playback": "hook.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Infinety-Utils/clappr-youtube-playback.git"
  },
